@using DAL_ID_
@model IEnumerable<ApplicataionUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<div class="panel panel-primary">
    <div class="panel-heading">
        Users
    </div>
    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>user</th>
            <th>mail</th>
        </tr>
        @if (Model.Count() == 0)
        {
            <tr>
                <td>a</td>
                <td>a</td>
                <td>a</td>
            </tr>
        }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                </tr>
            }
        }

    </table>
</div>
